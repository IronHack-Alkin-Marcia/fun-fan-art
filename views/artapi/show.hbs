<section class="features" id="features">
  <div class="container">
    <div class="section-heading text-center">
      <h2>{{art.title}}</h2>
      <p class="text-muted">{{art.smallDescription}}</p>
      <hr>
    </div>
    <div class="row">
      <div class="col-lg-12 my-auto">
        <div class="container-fluid">
          <div class="row">

            <div><a href="/artapi/">Go back</a></div>


            <div>all the available fields</div>
            <div>artId: {{art.artId}}</div>
            <div>title*: {{art.title}}</div>
            <div>artist*: {{art.artist}}</div>
            <div>smallDescription: {{art.smallDescription}}</div>
            <div>description*: {{art.description}}</div>
            <div>img: {{art.img}}</div>
            <div>imgWidth: {{art.imgWidth}}</div>
            <div>imgHeight: {{art.imgHeight}}</div>
            <div>year*: {{art.year}}</div>
            <div>***<img src="{{art.img}}" alt="{{art.title}}" /></div>






          </div>

        </div>
      </div>
    </div>
  </div>
</section>